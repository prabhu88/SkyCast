{
  "name": "sky-cast",
  "version": "1.0.0",
  "description": "SkyCast: Your Personal Weather Assistant",
  "repository": "https://github.com/prabhu88/Invoice-Genie.git",
  "main": "./main.js",
  "homepage": "./",
  "scripts": {
    "start": "concurrently \"npm run react-start\" \"npm run server-start\" \"wait-on http://localhost:3000 && npm run electron-dev\"",
    "react-build": "react-app-rewired build",
    "react-start": "react-app-rewired start",
    "server-build": "babel server --out-dir build-server",
    "server-start": "nodemon --watch server --exec babel-node server/server.js",
    "electron-dev": "node scripts/dev.js scripts/start.js",
    "electron-pack": "electron-builder -c.extraMetadata.main='./main.js'",
    "pre-pack": "npm run react-build && npm run server-build"
  },
  "author": {
    "name": "S. Prabhu M.Tech",
    "email": "s.prabhu.mtech88@gmail.com"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "^7.4.4",
    "concurrently": "^4.1.0",
    "customize-cra": "^0.2.12",
    "electron": "^5.0.2",
    "electron-builder": "^20.41.0",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "3.0.1",
    "wait-on": "^3.2.0"
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "async": "^3.2.4",
    "axios": "^0.18.0",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.2.3",
    "cors": "^2.8.5",
    "electron-is-dev": "^1.1.0",
    "express": "^4.17.1",
    "node-sass": "4.12",
    "nodemon": "^1.19.1",
    "piping": "^1.0.0-rc.4",
    "react": "^16.8.6",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^16.8.6",
    "react-redux": "^8.0.5",
    "react-router-dom": "5",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.2",
    "scss": "^0.2.4",
    "styled-component": "^2.8.0"
  },
  "build": {
    "appId": "com.electron-react-node",
    "files": [
      "build/**/**/*",
      "build-server/**/**/*",
      "node_modules/**/*",
      "./main.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": []
}
